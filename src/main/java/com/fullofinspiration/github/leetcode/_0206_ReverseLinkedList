package com.fullofinspiration.github.leetcode;

public class _0206_ReverseLinkedList {
    class Solution {
    public ListNode reverseList(ListNode head) {
        ListNode dummy = new ListNode();
        while (head != null) {
            ListNode tmp = head.next;
            head.next = dummy.next;;
            dummy.next = head;
            head = tmp;
        }
        return dummy.next;
    }
  }
}
